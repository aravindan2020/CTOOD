package inheritance2;

public class B1 extends A1 {
	int b=10;
	
	 B1()
      {	
      super(5);
      System.out.println("this is subslass constructor");
      }
   void CSE()
   {
	   System.out.println("this is sub class method");
   }
   void PrintAttribute()
   {
	   System.out.printf("the base class b value is %d and sub class b value is %d",super.b,b);
   }
   
   void printoverride()
   {
	   CSE();
	   super.CSE();
   }
}
